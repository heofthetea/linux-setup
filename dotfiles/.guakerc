[general]
compat-delete='delete-sequence'
display-n=0
display-tab-names=0
gtk-use-system-default-theme=true
hide-tabs-if-one-tab=true
history-size=1000
infinite-history=true
lazy-losefocus=false
max-tab-name-length=100
mouse-display=true
open-tab-cwd=true
prompt-on-quit=true
quick-open-command-line='gedit %(file_path)s'
quick-open-enable=false
restore-tabs-notify=true
restore-tabs-startup=true
save-tabs-when-changed=true
scroll-keystroke=true
use-default-font=true
use-popup=true
use-scrollbar=true
use-trayicon=true
window-halignment=0
window-height=60
window-losefocus=true
window-refocus=false
window-tabbar=true
window-vertical-displacement=0
window-width=62

[keybindings/global]
show-focus='<Primary><Alt>5'
show-hide='F1'

[keybindings/local]
focus-terminal-down='<Shift><Alt>Down'
focus-terminal-left='<Shift><Alt>Left'
focus-terminal-right='<Shift><Alt>Right'
focus-terminal-up='<Shift><Alt>Up'
quit='<Primary>q'
split-tab-horizontal='<Primary>backslash'
split-tab-vertical='<Primary><Shift>percent'

[style/background]
transparency=90

[style/font]
allow-bold=true
palette='#000000000000:#FFFF55555555:#5050FAFA7B7B:#F1F1FAFA8C8C:#BDBD9393F9F9:#FFFF7979C6C6:#8B8BE9E9FDFD:#BBBBBBBBBBBB:#555555555555:#FFFF55555555:#5050FAFA7B7B:#F1F1FAFA8C8C:#BDBD9393F9F9:#FFFF7979C6C6:#8B8BE9E9FDFD:#FFFFFFFFFFFF:#F8F8F8F8F2F2:#1E1E1F1F2828'
palette-name='Dracula'
